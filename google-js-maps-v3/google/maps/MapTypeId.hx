package google.maps;
#if haxe3
import js.html.*;
#else
import js.Dom;
#end

extern class MapTypeId {

/*
This map type displays satellite images.
*/
public static var SATELLITE : Dynamic;

/*
This map type displays a normal street map.
*/
public static var ROADMAP : Dynamic;

/*
This map type displays a transparent layer of major streets on
satellite images.
*/
public static var HYBRID : Dynamic;

/*
This map type displays maps with physical features such as terrain
and vegetation.
*/
public static var TERRAIN : Dynamic;


}
